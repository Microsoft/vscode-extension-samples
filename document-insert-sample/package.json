{
	"name": "document-insert-sample",
	"displayName": "Document Insert Sample",
	"description": "Insert random words.",
	"publisher": "ElecTreeFrying",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.insertRandom",
		"onCommand:extension.insertRandomSelection",
		"onCommand:extension.deleteRandom",
		"onCommand:extension.deleteSelection",
		"onCommand:extension.deleteSelectionLine"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.insertRandom",
				"title": "Insert Random Word"
			},
			{
				"command": "extension.insertRandomSelection",
				"title": "Insert Random Word in Selected Line"
			},
			{
				"command": "extension.deleteRandom",
				"title": "Delete Random Word"
			},
			{
				"command": "extension.deleteSelection",
				"title": "Delete Selected Text"
			},
			{
				"command": "extension.deleteSelectionLine",
				"title": "Delete Text in Selected Line"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chance": "^1.0.9",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"chance": "^1.1.4"
	}
}
